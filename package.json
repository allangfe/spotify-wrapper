  "bugs": {
    "url": "https://github.com/allangfe/js-tdd-course/issues"
  },
  "homepage": "https://github.com/allangfe/js-tdd-course#readme",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "husky": "^0.15.0-rc.8",
